-- Create the third tab (Info)
local InfoTab = Window:MakeTab({
    Name = "INFO",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Info Tab Buttons
InfoTab:AddButton({
    Name = "Sebb - The Creator",
    Callback = function()
        print("Creator: Sebb")
    end
})

InfoTab:AddButton({
    Name = "Current version: 1.5",
    Callback = function()
        print("Version: 1.5")
    end
})

InfoTab:AddButton({
    Name = "YouTube: Sebb",
    Callback = function()
        local link = "https://youtube.com/@sebb-k9x?si=NuMivqb0pfgxOs4o"
        if setclipboard then
            setclipboard(link) -- For PC
        else
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "Copied!",
                Text = "The YouTube link has been copied to your clipboard.",
                Duration = 5
            })
        end
    end
})

InfoTab:AddButton({
    Name = "Discord: SEBB RBLX COMMUNITY",
    Callback = function()
        local link = "https://discord.gg/WH8Z7m2yuu"
        if setclipboard then
            setclipboard(link) -- For PC
        else
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "Copied!",
                Text = "The Discord Invite has been copied to your clipboard.",
                Duration = 5
            })
        end
    end
})

-- Initialize the library
OrionLib:Init()
